{
    "name": "spatie/laravel-export",
    "description": "Create a static site bundle from a Laravel app",
    "keywords": [
        "spatie",
        "laravel-export"
    ],
    "homepage": "https://github.com/spatie/laravel-export",
    "license": "MIT",
    "authors": [
        {
            "name": "Sebastian De Deyne",
            "email": "sebastian@spatie.be",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "guzzlehttp/guzzle": "^7.2",
        "illuminate/console": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/http": "^8.0",
        "illuminate/support": "^8.0",
        "psr/http-message": "^1.0",
        "spatie/crawler": "^5.0 || ^6.0",
        "symfony/console": "^4.2 || ^5.0",
        "symfony/http-foundation": "^4.2 || ^5.0",
        "symfony/process": "^4.2 || ^5.0",
        "symfony/psr-http-message-bridge": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^8.0 || ^9.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\Export\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\Export\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\Export\\ExportServiceProvider"
            ]
        }
    }
}
